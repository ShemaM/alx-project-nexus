{
  "name": "byn-k-platform",
  "version": "1.0.0",
  "packageManager": "pnpm@9.15.4",
  "description": "A Next.js frontend with a Django backend.",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9"
  },
  "scripts": {
    "build": "next build",
    "dev": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" next dev",
    "devsafe": "if exist .next rd /s /q .next && cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" next dev",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "1.1.3",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.563.0",
    "next": "15.4.11",
    "next-auth": "^4.24.11",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.71.1",
    "sharp": "0.34.2"
  },
  "devDependencies": {
    "@playwright/test": "1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/react": "16.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.5.4",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.7",
    "jsdom": "26.1.0",
    "next": "15.4.11",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
